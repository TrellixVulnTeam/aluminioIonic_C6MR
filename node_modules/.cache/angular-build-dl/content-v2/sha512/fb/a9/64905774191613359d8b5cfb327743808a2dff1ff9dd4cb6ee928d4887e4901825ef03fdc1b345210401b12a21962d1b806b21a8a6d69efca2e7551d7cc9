  }\r\n  perfil(){\r\n    this.router.navigate(['/perfil']);\r\n  }\r\n\r\n  doRefresh(event) {\r\n    setTimeout(() => {\r\n      console.log('Actualizando Productos...');\r\n      this.getProducts();\r\n      event.target.complete();\r\n    }, 2000);\r\n  }\r\n\r\n}\r\n"]}