rt.present();\r\n  }\r\n  perfil(){\r\n    this.router.navigate(['/perfil']);\r\n  }\r\n\r\n  getUser(){\r\n    this.logs = JSON.parse(localStorage.getItem('Usuario'));\r\n    \r\n    this.auth.getUsers('usuariosStorage/', this.logs).subscribe((res) =>{ \r\n      this.usuarios$ = res;\r\n      let user_id =  this.usuarios$.id;\r\n      this.getFavoritos(user_id);\r\n    });\r\n  }\r\n}\r\n"]}